{
  "name": "gtm-brain-slack-bot",
  "version": "1.0.0",
  "description": "Elite GTM Intelligence Agent - Conversational Salesforce data assistant for Slack",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "setup": "node setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test:connections": "node test-connection.js",
    "docker:build": "docker build -t gtm-brain .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "health": "curl -f http://localhost:3000/health"
  },
  "keywords": [
    "salesforce",
    "slack",
    "gtm",
    "sales",
    "ai",
    "bot"
  ],
  "author": "Your Company",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@slack/bolt": "^3.17.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "cookie-parser": "^1.4.6",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.11.0",
    "jsforce": "^2.0.0-beta.28",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.0.379",
    "redis": "^4.6.10",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
